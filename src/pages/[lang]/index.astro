---
export const prerender = true;
import HomeContent from '~/components/HomeContent.astro';

export function getStaticPaths() {
  return [
    { params: { lang: 'en' } },
    { params: { lang: 'zh' } },
    { params: { lang: 'zh-tw' } },
    { params: { lang: 'ja' } },
    { params: { lang: 'ko' } },
  ];
}

const { lang } = Astro.params;
---
<HomeContent lang={lang} />
